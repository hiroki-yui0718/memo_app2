<div class="row">
<div class="col-md-6">
<h1>Log In</h1>
<%= form_with scope: :session,local:true do |f| %>
<%= f.label :email,"メールアドレス" %>
<%= f.text_field :email,class:"form-control" %>
<%= f.label :password,"パスワード" %>
<%= f.password_field :password,class:"form-control" %>
<%= f.submit "ログイン",class:"mt-3 btn btn-primary" %>
<% end %>
<%= link_to "#",class:"mt-3 btn btn-primary btn-block" do %>
<%= fa_icon("twitter") %> twitter To Login
<% end %>
<%= link_to "#",style:"background-color:#305097;",class:"btn btn-block" do %>
<%= fa_icon "facebook",style:"color:white;" %><span class="text-light ml-2">facebook To Login</span>
<% end %>
</div>
</div>